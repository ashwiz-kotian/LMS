<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminlogin">

    <ImageView
        android:id="@+id/img_id"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/main"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/adminemail"
        android:layout_width="363dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/recatngle_3"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:textAlignment="viewStart"
        android:textColorHint="#EFDCDC"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.271" />

    <EditText
        android:id="@+id/adminpassword"
        android:layout_width="357dp"
        android:layout_height="69dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/recatngle_3"
        android:hint="Password"
        android:inputType="textPassword"
        android:textColorHint="#ECE4E4"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.489" />

    <View
        android:id="@+id/line_1"
        android:layout_width="355dp"
        android:layout_height="4dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/line_1"
        app:layout_constraintBottom_toBottomOf="@+id/adminemail"
        app:layout_constraintEnd_toEndOf="@+id/adminemail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/adminemail"
        app:layout_constraintTop_toTopOf="@+id/adminemail"
        app:layout_constraintVertical_bias="0.893" />

    <View
        android:id="@+id/line_2"
        android:layout_width="349dp"
        android:layout_height="4dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/line_2"
        app:layout_constraintBottom_toBottomOf="@+id/adminpassword"
        app:layout_constraintEnd_toEndOf="@+id/adminpassword"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/adminpassword"
        app:layout_constraintTop_toTopOf="@+id/adminpassword"
        app:layout_constraintVertical_bias="0.878" />
    <ImageView
        android:id="@+id/vector"
        android:layout_width="32dp"
        android:layout_height="25dp"
        android:background="@drawable/useremail"
        app:layout_constraintBottom_toBottomOf="@+id/adminemail"
        app:layout_constraintEnd_toEndOf="@+id/adminemail"
        app:layout_constraintHorizontal_bias="0.931"
        app:layout_constraintStart_toStartOf="@+id/adminemail"
        app:layout_constraintTop_toTopOf="@+id/adminemail"
        app:layout_constraintVertical_bias="0.434"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/vector2"
        android:layout_width="29dp"
        android:layout_height="32dp"
        android:background="@drawable/userpassword"
        app:layout_constraintBottom_toBottomOf="@+id/line_2"
        app:layout_constraintEnd_toEndOf="@+id/adminpassword"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintStart_toStartOf="@+id/adminpassword"
        app:layout_constraintTop_toTopOf="@+id/adminpassword"
        app:layout_constraintVertical_bias="0.548"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/adminloginbtn"
        android:layout_width="208dp"
        android:layout_height="74dp"
        android:text="LOGIN"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.467"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.719" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="300dp"
        android:layout_height="44dp"
        android:text="ADMIN LOGIN"
        android:textAlignment="center"
        android:textColor="#E4E40B"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.08" />

</androidx.constraintlayout.widget.ConstraintLayout>